	RewriteRule ^news/([^/.]+)/?$ noticia.php?slug=$1 [L]
	RewriteRule ^contact/?$ contact.php [L]
	RewriteRule ^contact/thanks/?$ contact.php?msg=1 [L]
	RewriteRule ^safety-protocols/?$ safety.php [L]
	RewriteRule ^privacy-policy/?$ privacy.php [L]
